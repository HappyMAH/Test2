<Page x:Class="Capcha.Pages.FirstPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Capcha.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="FirstPage">

    <Grid>
        <StackPanel Width="800" Height="300">
        <StackPanel Orientation="Horizontal" x:Name="Symbols" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10" Background="White" > </StackPanel>
            <Canvas  x:Name="CanvasCV" HorizontalAlignment="Center" VerticalAlignment="Bottom" >
                
            </Canvas>
            <Button x:Name="CapchaBtn" Click="CapchaBtn_Click" Content="Обновить капчу" Width="150" Height="25"  VerticalAlignment="Bottom" ></Button>
        <Button x:Name="AddCRBtn" Click="AddCRBtn_Click" Content="Клиенты" Width="150" Height="25"  VerticalAlignment="Bottom" ></Button>
            <Button x:Name="AddAgentBtn" Click="AddAgentBtn_Click" Content="Агенты" Width="150" Height="25"  VerticalAlignment="Bottom" ></Button>
       </StackPanel>

        
        
    </Grid>
</Page>
