<Page x:Class="Capcha.Pages.AddAgentPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Capcha.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AddAgentPage">

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
            <Label>Имя</Label>
            <TextBox TextChanged="FirstNameAgentTB_TextChanged" x:Name="FirstNameAgentTB" Width="150" Height="25"></TextBox>
            <Label>Фамилия</Label>
            <TextBox TextChanged="MiddleNameAgentTB_TextChanged" x:Name="MiddleNameAgentTB" Width="150" Height="25"></TextBox>
            <Label>Отчество</Label>
            <TextBox TextChanged="LastNameAgentTB_TextChanged" x:Name="LastNameAgentTB" Width="150" Height="25"></TextBox>
            <Label>Доля</Label>
            <TextBox TextChanged="DealShareTB_TextChanged" x:Name="DealShareTB" Width="150" Height="25"></TextBox>
            
            
            <Button x:Name="AddAgentBtn" Click="AddAgentBtn_Click" Content="Добавить агента" Width="120"></Button>
        </StackPanel>
    </Grid>
</Page>
